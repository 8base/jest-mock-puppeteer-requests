{
  "name": "jest-mock-puppeteer-requests",
  "version": "0.0.1-test",
  "main": "lib/index.js",
  "module": "src/index.js",
  "scripts": {
    "build": "NODE_ENV=production babel src -d lib --ignore __tests__",
    "test": "jest",
    "watch": "NODE_ENV=development babel src -d lib --watch --ignore __tests__"
  },
  "dependencies": {
    "mkdirp": "^0.5.1",
    "ramda": "^0.25.0",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "jest": "^23.6.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**",
      "!<rootDir>/**/__tests__/**"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js"
    ]
  },
  "license": "MIT"
}
